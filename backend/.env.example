# Server Configuration
PORT=5000
NODE_ENV=development

# Database
MONGODB_URI=mongodb://localhost:27017/activelearn

# API Keys
YOUTUBE_API_KEY=your_youtube_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Google Custom Search API (for article scraping)
# You need 2 API keys to handle rate limiting
GOOGLE_SEARCH_API_KEY_1=your_google_search_api_key_1_here
GOOGLE_SEARCH_API_KEY_2=your_google_search_api_key_2_here
GOOGLE_SEARCH_ENGINE_ID=your_custom_search_engine_id_here

# JWT Secret
JWT_SECRET=your_jwt_secret_here
